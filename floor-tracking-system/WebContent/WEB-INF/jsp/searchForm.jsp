<script type="text/javascript" src="../js/searchForm.js"></script>
<div style="text-align:center;">
<div class="divTable">
	<div class="tableTitle">Search Employees</div>
	<div class="divRow"><div class="divCol"><div class="left bold">Name</div><div class="right"><input type="text" name="name" id="srchname"/></div></div></div>
	<div class="divRow"><div class="divCol"><div class="left bold">Employee Id</div><div class="right"><input type="text" name="pass" id="srchempid"/></div></div></div>
	<div class="divRow"><div class="divCol"><div class="left bold">Asset Id</div><div class="right"><input type="text" name="user" id="srchmachineno"/></div></div></div>
	<div class="divRow"><div class="divCol"><div class="left bold">Cube No</div><div class="right"><input type="text" name="user" id="srchncubeno"/></div></div></div>
	<div class="divRow"><div class="divCol"><div class="left bold">Project</div><div class="right"><input type="text" name="user" id="srchproject"/></div></div></div>
	<div class="divRow"><div class="divCol"><div class="left bold">Show Full Details</div><div class="right"><input type="checkbox" id="srchdetailview"></div></div></div>
	<div class="divRow">
		<div class="divCol">
		<fieldset class="border1">
			<legend class="bold">Shift</legend>
			<span><input type="checkbox" id="srchShiftA">A - Morning</span>
			<span><input type="checkbox" id="srchShiftB">B - Afternoon</span>
			<span><input type="checkbox" id="srchShiftC">C - Night</span>
			<span><input type="checkbox" id="srchShiftD">D - General</span>
		</fieldset>
		</div>
	</div>
	<div class="divRow">
		<div class="divCol">
			<div class="left"><input type="button" class="g-button"  value="Submit" id="srchBtn"/></div>
			<div><input type="reset" class="g-button"  value="Clear" id="clrSrch"/></div>
		</div>
	</div>
</div>
</div>

